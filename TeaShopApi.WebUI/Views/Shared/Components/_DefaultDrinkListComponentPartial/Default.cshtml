@model List<TeaShopApi.WebUI.Dtos.DrinkDtos.ResultDrinkDtos>

<div class="services_section layout_padding">
    <div class="container">
        <h1 class="services_taital">İçecek Hizmetlerimiz</h1>
        <p class="services_text">Uzun yıllardır aynı lezzet </p>
        <div class="services_section_2 layout_padding">
            <div class="row">
				@foreach (var item in Model)
				{
                <div class="col-md-6">   
                    <div class="box_main ">
                            <div class="left_main" style="height:200px; width:150px;">
                                <div class="cup_img_1" style="height:200px;">
									<img src="@item.drinkImageUrl">
								</div>
                        </div>
                        
                        <div class="right_main">
                            <h6 class="milk_text">@item.drinkName</h6>
                            <h1 class="price_text">@item.drinkPrice ₺</h1>
                        </div>
                    </div>
                    
                </div>
					}
            </div>
        </div>
        <div class="see_bt"><a href="#">Tüm İçecekleri Göre</a></div>
    </div>
</div>
